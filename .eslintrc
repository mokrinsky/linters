{
  "extends": "standard",
  "env": {
      "es6": true,
      "browser": true,
      "node": true
  },
  "rules": {
      "indent": [2, 2, {"SwitchCase": 1}],
      "quotes": [2, "double"],
      "no-spaced-func": 0,
      "no-unexpected-multiline": 0,
      "one-var": 0,
      "no-control-regex": 0,
      "new-cap": 0,
      "no-unused-vars": [2, { "args": "none" }]
  }
}
